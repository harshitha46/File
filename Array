

#include <stdio.h>
void main()
{
    int i,n,a[5];
    printf("enter the size of array:");
    scanf("%d",&n);
    printf("enter the array elements:");
    for(i=1;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("array elements are: ");
    for(i=0;i<n;i++)
{
    printf("%d",a[i]);
}
}
